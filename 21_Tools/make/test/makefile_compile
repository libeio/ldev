
# 命令参数变量

# 运行命令 make CFLAGS=-Wall

#main : hello.o
#	$(CXX) -std=c++11 -o hello hello.o

# 只生成中间文件  C++11 好像无法使用隐含规则
object : std_function.cpp
	$(CXX) -std=c++11 std_function.cpp -c -o std_function.o

# 可执行文件	
main : std_function.o
	$(CXX) -std=c++11 std_function.o -o std_function
	
.PHONY: clean
clean:
	@$(RM) std_function.o std_function
